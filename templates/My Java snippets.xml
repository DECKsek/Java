<templateSet group="My Java snippets">
  <template name="println" value="System.out.println($END$);" description="Java - Print and new line" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="print" value="System.out.print($END$);" description="Java - Print" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="while" value="while ($CNOD$) {&#10;    $CODE$&#10;}" description="while loop" toReformat="false" toShortenFQNames="true">
    <variable name="CNOD" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="CODE" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="if" value="if ($COND$) {&#10;    $CODE$&#10;}" description="java - if condition" toReformat="false" toShortenFQNames="true">
    <variable name="COND" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="CODE" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="foreach" value="for ($ITER$ : $COLLECTION$) {&#10;    $CODE$&#10;}" description="Enchanced For - Java" toReformat="false" toShortenFQNames="true">
    <variable name="ITER" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="COLLECTION" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="CODE" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
</templateSet>